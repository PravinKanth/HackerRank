import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        String s=sc.next();
        String p="";
        
       
        for(int i=s.length()-1;i>=0;i--){
            p+=(s.charAt(i));
        }
        
        if(p.equals(s)){
            System.out.print("Yes");
        }
        else{
            System.out.print("No");
            
        }
    }
}
