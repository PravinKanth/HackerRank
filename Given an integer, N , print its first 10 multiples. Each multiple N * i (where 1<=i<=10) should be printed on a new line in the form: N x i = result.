import java.io.*;
import java.math.*;
import java.security.*;
import java.text.*;
import java.util.*;
import java.util.concurrent.*;
import java.util.regex.*;



public class Solution {
    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        int i=s.nextInt();
        for(int a=1;a<=10;a++){
            System.out.println(i+" x "+a+" = "+(i*a));
            
        }
        
    }
}
