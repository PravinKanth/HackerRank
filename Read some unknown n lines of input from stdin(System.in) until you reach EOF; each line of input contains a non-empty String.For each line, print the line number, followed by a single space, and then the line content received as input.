import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
      Scanner sc=new Scanner(System.in);  
      int j=1;
      
      while (sc.hasNext()){
          String a=sc.nextLine();
          System.out.println(j+" "+a);
          j+=1;
      }
    }
}
