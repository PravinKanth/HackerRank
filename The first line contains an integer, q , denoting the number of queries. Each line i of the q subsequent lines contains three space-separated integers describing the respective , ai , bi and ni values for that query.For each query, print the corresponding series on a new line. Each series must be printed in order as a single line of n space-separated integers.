import java.util.*;
import java.io.*;
import java.lang.Math;

class Solution{
    public static void main(String []argh){
        Scanner in = new Scanner(System.in);
        int t=in.nextInt();
        for(int i=0;i<t;i++){
            int a = in.nextInt();
            int b = in.nextInt();
            int n = in.nextInt();
            int po=(int)(Math.pow(2,0));
            int d=a+po*b;
            System.out.print(d+" ");
            for(int k=1;k<n;k++){
            d+=(Math.pow(2,k))*b;  
            System.out.print(d+" ");
        }
        System.out.printf("\n");
        }
        in.close();
    }
}
